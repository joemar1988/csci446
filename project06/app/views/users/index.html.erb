%ul.menubar
  %li= link_to "login", 
  %li= link_to "register", new_user_path 
.heading
  %h1 Gamez Admin
%h2 Users
.body
  %table.complete
    %tr
      %th Name
      %th Username
      %th Last Login
	  %th Number of Games
    - @users.each do |article|
      %tr
        %td= h user.firstname user.lastname
        %td= h user.username
        %td= h article.updated_at.strftime("%A, %b. %d, %Y")
        = will_paginate @users